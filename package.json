{
  "name": "boba-tracker",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "cf:build": "opennextjs-cloudflare build",
    "cf:deploy": "bun run cf:build && wrangler deploy",
    "cf:deploy:prod": "bun run cf:build && wrangler deploy",
    "lint": "eslint",
    "format": "prettier \"{app,lib}/**/*.{ts,tsx,js,jsx,json,md,css}\" --write",
    "format:check": "prettier \"{app,lib}/**/*.{ts,tsx,js,jsx,json,md,css}\" --check"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.990.0",
    "bcryptjs": "^3.0.2",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.511.0",
    "mongodb": "^6.20.0",
    "next": "16.1.6",
    "react": "19.2.3",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "19.2.3"
  },
  "devDependencies": {
    "@opennextjs/cloudflare": "^1.16.5",
    "@tailwindcss/postcss": "^4",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "prettier": "^3.8.1",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "sharp": "^0.34.5",
    "tailwindcss": "^4",
    "typescript": "^5",
    "wrangler": "^4.65.0"
  },
  "ignoreScripts": [
    "unrs-resolver"
  ],
  "trustedDependencies": [
    "sharp",
    "unrs-resolver"
  ]
}
