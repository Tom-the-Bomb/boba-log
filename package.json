{
  "name": "boba-log",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build --webpack",
    "start": "next start",
    "preview": "opennextjs-cloudflare build -- --webpack && opennextjs-cloudflare preview -- --remote",
    "deploy": "opennextjs-cloudflare build -- --turbopack && opennextjs-cloudflare deploy -- --keep-vars",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts",
    "assets:process": "node scripts/normalize-default-shops.mjs",
    "lint": "eslint . && prettier --check . && tsc --noEmit",
    "lint:fix": "eslint --fix . && prettier --write . && tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky",
    "i18n:upload": "simplelocalize upload --dryRun",
    "i18n:upload:confirm": "simplelocalize upload",
    "i18n:download": "simplelocalize download"
  },
  "dependencies": {
    "@marsidev/react-turnstile": "^1.4.2",
    "@tsndr/cloudflare-worker-jwt": "^3.2.1",
    "i18next": "^25.8.10",
    "lucide-react": "^0.511.0",
    "next": "16.1.6",
    "next-themes": "^0.4.6",
    "react": "19.2.3",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "19.2.3",
    "react-i18next": "^16.5.4",
    "sonner": "^2.0.7"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20260214.0",
    "@opennextjs/cloudflare": "^1.16.5",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "schema-dts": "^1.1.5",
    "tailwindcss": "^4",
    "typescript": "^5",
    "wrangler": "^4.65.0"
  },
  "ignoreScripts": [
    "unrs-resolver"
  ],
  "trustedDependencies": [
    "unrs-resolver"
  ],
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "reactDoctor": {
    "ignore": {
      "files": [
        "open-next.config.ts"
      ]
    }
  }
}
